
# Ignore the vendor directory (Composer dependencies)
vendor/

# Ignore Composer lock file if you prefer not to lock the dependencies (not recommended)
# composer.lock

# Ignore environment configuration files (for local development)
.env
.env.local
.env.*.local

# Ignore log files and temporary files
*.log
*.sql
*.sqlite
*.swp
*.swo

# Ignore operating system-specific files
.DS_Store   # macOS specific
Thumbs.db   # Windows specific

# Ignore IDE/Editor configuration files
.vscode/        # VS Code directory
.idea/          # PhpStorm / IntelliJ IDEA directory
*.sublime-project
*.sublime-workspace

# Ignore PHP unit test result files
phpunit.result.cache

# Ignore temporary or backup files generated by editors
*~             # Emacs or Vim backup files
*.bak          # Backup files
*.tmp          # Temporary files

# Ignore compiled files (such as bytecode and cache)
*.php.bin      # PHP bytecode
*.phar          # PHP Archive (PHAR)
.cache/         # Cache directories

# Ignore node_modules if your project has a Node.js environment
node_modules/

# Ignore package manager files if you are using Yarn or npm (optional)
yarn.lock
package-lock.json

# Ignore PHPUnit code coverage reports
coverage/
build/

# Ignore Docker-related files
docker-compose.override.yml
.docker/

# Ignore test and build output directories
/tests/
.build/
dist/

# Ignore custom config files for different environments
/config/*.local
/config/*.dev
/config/*.prod

# Ignore Symfony app and cache directories (if using Symfony)
var/
