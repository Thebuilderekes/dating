{
    "require": {
        "vlucas/phpdotenv": "^5.6"
    },

    "autoload-dev": {
    "psr-4": {
        "App\\": "app/",
        "Tests\\": "tests/"
    }
},
    "require-dev": {
        "pestphp/pest": "^3.8",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts":{
        "stan" : "vendor/bin/phpstan analyse -c phpstan.neon"
    }

}
